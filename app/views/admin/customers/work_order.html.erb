<%= semantic_form_for [:admin, @invoice] do |f| %>
  <%= f.inputs :name => "Work Order Information" do %>
    <%= f.input :service_date, :as => :just_datetime_picker %>
  <% end %>

  <table class="index_table">
    <thead>
      <tr><th>Service</th><th>Notes</th><th>Price</th></tr>
    </thead>
    <tbody>
      <% @service_types.each do |service_type| -%>
      <tr><td><%= service_type.name %></td><td></td><td></td></tr>
      <% end -%>
    </tbody>
  </table>
  <%= f.actions %>
<% end %>
